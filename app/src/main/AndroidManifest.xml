<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cskh.tanhoa.com.ditagis.hcm.cskh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TrangChuActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TraCuuActivity"
            android:label="@string/tra_cuu_activity"
            android:parentActivityName=".TrangChuActivity" />
        <activity
            android:name=".tracuu.TraCuuTienNuocActivity"
            android:label="@string/tra_cuu_tien_nuoc_activity"
            android:parentActivityName=".TraCuuActivity" />
        <activity android:name=".LoginActivity"></activity>
    </application>

</manifest>